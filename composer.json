{
    "name": "scottybo/laravel-facebook-sdk",
    "description": "Fully unit tested Facebook SDK v5 integration for Laravel 6.x",
    "keywords": ["facebook", "laravel", "lumen", "sdk", "facebook sdk v4", "facebook sdk v5"],
    "homepage": "https://github.com/scottybo/laravel-facebook-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Scott Bowler",
            "email": "scott.bowler@dcsworldwide.net",
            "homepage": "https://www.dcsworldwide.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/auth": "^6",
        "illuminate/config": "^6",
        "illuminate/database": "^6",
        "illuminate/routing": "^6",
        "illuminate/session": "^6",
        "illuminate/support": "^6",
        "facebook/graph-sdk": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0|~5.0|~6.0",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Scottybo\\LaravelFacebookSdk\\": "src/LaravelFacebookSdk"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Scottybo\\LaravelFacebookSdk\\Test\\": "tests"
        }
    },
    "branch-alias": {
        "dev-master": "4.x-dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Scottybo\\LaravelFacebookSdk\\LaravelFacebookSdkServiceProvider"
            ],
            "aliases": {
                "Facebook": "Scottybo\\LaravelFacebookSdk\\FacebookFacade"
            }
        }
    }
}
